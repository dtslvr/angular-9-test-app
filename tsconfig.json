{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noLib": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es2015",
    "lib": ["es2017", "dom"],
    "module": "esnext",
    "paths": {
      "@sublimd/app/*": ["./src/app/*"],
      "@sublimd/env/*": ["./src/environments/*"]
    }
  },
  "files": ["./src/main.ts", "./src/polyfills.ts"]
}
